<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
	<title><![CDATA[MLS Finder WordPress Plugin]]></title>
	<parser>
		<default-package-name>root</default-package-name>
		<target>../docs</target>
	</parser>
	<transformer>
		<target>../docs</target>
	</transformer>
	<files>
		<directory>.</directory>
		<ignore>tests/*,</ignore>
		<ignore>ant/*</ignore>
		<ignore>js/*</ignore>
		<ignore>css/*</ignore>
	</files>
	<logging>
		<level>debug</level>
		<paths>
			<default>{APP_ROOT}../docs/log/{DATE}.log</default>
			<errors>{APP_ROOT}../docs/log/{DATE}.errors.log</errors>
		</paths>
	</logging>
	<plugins>
		<plugin path="Core">
			<option name="a">value</option>
		</plugin>
	</plugins>
</phpdocumentor>
